Xft*dpi: 70

URxvt.foreground: white
URxvt.background: rgba:0000/0000/0000/cccc
URxvt.color0: #2e3436
URxvt.color1: #cc0000
URxvt.color2: #4e9a06
URxvt.color3: #c4a000
URxvt.color4: #3465a4
URxvt.color5: #75507b
URxvt.color6: #06989a
URxvt.color7: #d3d7cf
URxvt.color8: #555753
URxvt.color9: #ef2929
URxvt.color10: #8ae234
URxvt.color11: #fce94f
URxvt.color12: #729fcf
URxvt.color13: #ad7fa8
URxvt.color14: #34e2e2
URxvt.color15: #eeeeec
URxvt.depth: 32
URxvt.hilightColor: white

URxvt.iconFile: /usr/share/pixmaps/gnome-terminal.png

URxvt.font: xft:DejaVu Sans Mono-13:autohint=true,xft:Bitstream Vera Sans Mono-13:autohint=true
URxvt.boldFont: xft:DejaVu Sans Mono-13:bold:autohint=true,xft:Bitstream Vera Sans Mono-13:bold:autohint=true
URxvt.italicFont: xft:DejaVu Sans Mono-13:italic:autohint=true,xft:Bitstream Vera Sans Mono-13:italic:autohint=true
URxvt.boldItalicFont: xft:DejaVu Sans Mono-13:bold:italic:autohint=true,xft:Bitstream Vera Sans Mono-13:bold:italic:autohint=true

URxvt.scrollBar: false
URxvt.saveLines: 8192
URxvt.secondaryScroll: true

URxvt.perl-ext: default,matcher
URxvt.urlLauncher: xdg-open
URxvt.matcher.button: 1
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]

Fig.latexfonts: true
Fig.specialtext: true

xterm*customization: -color
xterm*background: black
xterm*foreground: white
! xterm*Color0: #2e3436
xterm*Color1: #cc0000
! xterm*Color2: #4e9a06
xterm*Color3: #c4a000
xterm*Color4: #3465a4
xterm*Color5: #75507b
! xterm*Color6: #06989a
! xterm*Color7: #d3d7cf
! xterm*Color8: #555753
xterm*Color9: #ef2929
! xterm*Color10: #8ae234
xterm*Color11: #fce94f
xterm*Color12: #729fcf
xterm*Color13: #ad7fa8
! xterm*Color14: #34e2e2
! xterm*Color15: #eeeeec
xterm*FaceName: "Deja Vu Sans Mono"
xterm*FaceSize: 13
xterm*TermName: xterm-256color
xterm*Utf8: 1
xterm*MetaSendsEscape: true
xterm*SaveLines: 8192
xterm*TrimSelection: true
